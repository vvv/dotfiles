#!/usr/bin/expect -f

## Use example:
##   pass.exp net -S 192.168.2.100 rpc shutdown -C. -t1 < ~/.winpass

eval "spawn -noecho $argv"
expect password: {
    sleep 0.1; # `ioctl' may need some time to clear input buffer

    stty -echo
    expect_user -re "(.*)\n"
    stty echo

    send "$expect_out(1,string)\r"
}
expect eof
