[user]
	name = Valery V. Vorotyntsev
	email = valery.vv@gmail.com
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[alias]
	sb = show-branch
	fea = fetch -p --all
	fe = fetch -p
	st = status -sb
	desc = describe
	rp = rev-parse --short
	log1 = log --oneline
	rfm = push origin HEAD:refs/for/master
	sbo = show-branch origin/master HEAD
[rerere]
	enabled = true
